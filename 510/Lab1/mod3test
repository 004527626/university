S_00753E20 .scope module, "mod3counter_test" "mod3counter_test" 2 3;
 .timescale 0 0;
v0075FEE8_0 .var "num_in", 1 0;
v0075FF40_0 .net "num_out", 1 0, v00753890_0; 1 drivers
