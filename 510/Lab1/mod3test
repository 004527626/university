E_00759C10 .event edge, v007538E8_0;
    .scope S_00753808;
T_0 ;
    %wait E_00759C10;
    %load/v 8, v007538E8_0, 2;
    %mov 10, 0, 30;
    %addi 8, 1, 32;
    %movi 40, 3, 32;
    %mod 8, 40, 32;
    %set/v v00753890_0, 8, 2;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_00753E20;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "mod3.vcd";
    %vpi_call 2 14 "$dumpvars";
    %movi 8, 2, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0075FEE8_0, 0, 8;
    %delay 10, 0;
    %movi 8, 1, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0075FEE8_0, 0, 8;
    %delay 10, 0;
    %ix/load 0, 2, 0;
    %assign/v0 v0075FEE8_0, 0, 0;
    %delay 10, 0;
    %movi 8, 1, 2;
    %ix/load 0, 2, 0;
    %assign/v0 v0075FEE8_0, 0, 8;
    %delay 10, 0;
    %end;
    .thread T_1;
